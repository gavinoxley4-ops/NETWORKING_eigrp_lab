policy-options {
    policy-statement ACCEPT-ALL {
        then accept;
    }
    policy-statement ADVERTISE-ALL {
        term 1 {
            from protocol [ direct rip static ];
            then accept;
        }
    }
    policy-statement RIP-EXPORT {
        term LOCAL {
            from protocol [ direct local ];
            then accept;
        }
    }
    policy-statement RIP-IMPORT {
        term ACCEPT-ALL {
            then accept;
        }
    }
    policy-statement accept-routes {
        then accept;
    }
    policy-statement advertise-rip-routes {
        term 1 {
            from protocol rip;
            then accept;
        }
        term 2 {
            from protocol direct;
            then accept;
        }
    }
    policy-statement direct-routes {
        term 1 {
            from protocol direct;
            then accept;
        }
    }
    policy-statement send-default {
        term 1 {
            from {
                route-filter 0.0.0.0/0 exact;
            }
        }
        then accept;
    }
    policy-statement send-direct {
        term 1 {
            from protocol direct;
            then accept;
        }
    }
    policy-statement send-static {
        term 1 {
            from protocol static;
            then accept;
        }
    }
    policy-statement static-routes {
        term 1 {
            from protocol static;
            then accept;
        }
    }
}
