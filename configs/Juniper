    rip {
        group TQ {
            export send-default;
            import accept-routes;
        }
        group EIGRP-INTERCONNECT {
            preference 100;
            export RIP-EXPORT;
            import RIP-IMPORT;
            neighbor ge-0/0/0.0;
            neighbor se-0/0/0.0;
        }
        group rip-group {
            export [ send-direct send-static ];
        }
        group INTERNAL {
